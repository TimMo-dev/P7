{
  "name": "programming-learning-tool",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "run-p dev:client dev:server",
    "dev:client": "vite --config .config/vite.config.ts",
    "dev:server": "npx tsx ./server/server.ts",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview --config .config/vite.config.ts",
    "test:unit": "vitest --config .config/vitest.config.ts",
    "build-only": "vite build --config .config/vite.config.ts",
    "type-check": "vue-tsc --build --force --config .config/tsconfig.app.json"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.6.0",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.1.5",
    "@vue-hero-icons/solid": "^1.7.2",
    "autoprefixer": "^10.4.20",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "pg": "^8.13.1",
    "pg-pool": "^3.7.0",
    "postcss": "^8.4.47",
    "tsx": "^4.19.1",
    "vue": "^3.4.29"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/express": "^5.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.5",
    "@types/pg": "^8.11.10",
    "@types/vue-router": "^2.0.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "jsdom": "^24.1.0",
    "npm-run-all2": "^6.2.6",
    "tailwindcss": "^3.4.13",
    "typescript": "~5.4.0",
    "vite": "^5.4.10",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vitest": "^1.6.0",
    "vue-router": "^4.4.5",
    "vue-tsc": "^2.0.21"
  }
}
