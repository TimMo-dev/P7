{
  "references": [
    {
      "path": "./.config/tsconfig.node.json"
    },
    {
      "path": "./.config/tsconfig.app.json"
    },
    {
      "path": "./.config/tsconfig.vitest.json"
    }
  ],
  "compilerOptions": {
   "lib": [
      "es5",
      "es6"
   ],
   "target": "es5",
   "module": "commonjs",
   "moduleResolution": "node",
   "outDir": "./build",
   "emitDecoratorMetadata": true,
   "experimentalDecorators": true,
   "sourceMap": true,
   "allowImportingTsExtensions": true,
   "noEmit": true
}
}
